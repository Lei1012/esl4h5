	"preloadRule": {
		"pages/home/index": {
			"network": "all",
			"packages": ["__APP__"]
		}
	},

	"globalStyle": {
		"pageOrientation": "portrait",
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "ESL Passport",
		"navigationBarBackgroundColor": "#0AA0A8",
		"backgroundColor": "#F4F5F6",
		"mp-360": {
			"navigationStyle": "custom"
		},
		"h5": {
			"titleNView": {
				"backgroundColor": "#0AA0A8",
				"type": "transparent"
			},
			"pullToRefresh": {
				"color": "#00B3D2"
			}
		}
	},
	"topWindow": {
		"path": "responsive/top.vue", // 指定 topWindow 页面文件
		"style": {
			"height": "0"
		},
		"matchMedia": {
			"minWidth": 768 //生效条件，当窗口宽度大于768px时显示
		}
	},
	"leftWindow": {
		"path": "responsive/left.vue", // 指定 leftWindow 页面文件
		"style": {
			"width": "calc((100% - 375px)/2)"
		},
		"matchMedia": {
			"minWidth": 768 //生效条件，当窗口宽度大于768px时显示
		}
	},
	"rightWindow": {
		"path": "responsive/right.vue", // 指定 rightWindow 页面文件
		"style": {
			"width": "calc((100% - 375px)/2)" // 页面宽度
		},
		"matchMedia": {
			"minWidth": 768 //生效条件，当窗口宽度大于768px时显示
		}
	},
	"tabBar": {
		"color": "#FFFFFF",
		"selectedColor": "#000000",
		"borderStyle": "black",
		"backgroundColor": "#0AA0A8",
		"height": "60px",
		"fontSize": "12px",
		"iconWidth": "24px",
		"spacing": "3px",
		"list": [{
				"pagePath": "pages/home/index",
				"iconPath": "static/home.png",
				"selectedIconPath": "static/home-fill.png",
				"text": "Home"
			},
